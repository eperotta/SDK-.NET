              <endpoint address="https://10.123.4.121:8243/services/t/1.1/Operations.OperationsHttpsSoap11Endpoint" binding="basicHttpBinding" bindingConfiguration="OperationsSoap11Binding" contract="OperationsService.OperationsPortType" name="OperationsHttpsSoap11Endpoint"/>

              <endpoint address="http://10.123.4.121:8280/services/t/1.1/Operations.OperationsHttpSoap12Endpoint" binding="customBinding" bindingConfiguration="OperationsSoap12Binding" contract="OperationsService.OperationsPortType" name="OperationsHttpSoap12Endpoint"/>

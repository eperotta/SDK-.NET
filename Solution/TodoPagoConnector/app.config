              <endpoint address="http://10.123.4.121:8280/services/t/1.1/Authorize.AuthorizeHttpSoap12Endpoint" binding="customBinding" bindingConfiguration="AuthorizeSoap12Binding" contract="AuthorizeService.AuthorizePortType" name="AuthorizeHttpSoap12Endpoint"/>
